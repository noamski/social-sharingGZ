{
  "name": "balloons-gz",
  "version": "1.0.0",
  "description": "Starter application of Angular4 with Express",
  "main": "server/app.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others \"npm run server:run\" \"ng serve --proxy-config proxy.conf.json\"",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "test:client": "ng test --watch false",
    "e2e:client": "ng e2e",
    "build": "ng build --prod && npm run server:build",
    "server:run": "nodemon",
    "server:build": "tsc -p ./server",
    "postinstall": "npm run build",
    "firebase-deploy": "ng build --prod && firebase deploy"
  },
